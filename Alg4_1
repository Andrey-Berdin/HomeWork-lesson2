public class Alg4_1 {
    public static void main(String[] args) {
        /*
            Дан массив целых чисел.
            Найти сумму массива.
         */
        int [] array = {12,10,40,21,34,28,67,59}; // Версия с заданным массивом целых чисел.
        int sum = 0;
        // Цекл для обхода каждого элемента
        for ( int num : array) {
            // суммирование каждого элемента массива
            sum = sum + num;
        }
        System.out.println("Сумма элементов массива равна : " + sum);
    }
}
